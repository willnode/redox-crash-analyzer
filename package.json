{
  "name": "redox-crash-analyzer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:emsdk": "cd emsdk && ./emsdk install latest && ./emsdk activate latest",
    "build:capstone": "env EMSCRIPTEN=$PWD/emsdk/upstream/emscripten bash -c '. ./emsdk/emsdk_env.sh && python3 build.py'",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@wokwi/elfist": "^1.0.2",
    "ts-capstone": "^0.1.6",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "babel-plugin-transform-amd-to-commonjs": "^1.6.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-commonjs": "^0.10.4",
    "vue-tsc": "^3.1.0"
  }
}
